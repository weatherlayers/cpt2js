{
  "name": "cpt2js",
  "version": "1.6.2",
  "description": "Color palette text parser to a function, input compatible with GMT, GDAL, GRASS, PostGIS, ArcGIS",
  "keywords": [
    "color",
    "palette",
    "colormap",
    "cpt",
    "gmt",
    "gdal",
    "grass",
    "postgis",
    "arcgis"
  ],
  "author": "WeatherLayers.com <info@weatherlayers.com>",
  "license": "MPL-2.0",
  "repository": "github:weatherlayers/cpt2js",
  "files": [
    "LICENSE.md",
    "dist/**/*.cjs",
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/cpt2js.d.ts",
      "script": "./dist/cpt2js.umd.min.js",
      "import": "./dist/cpt2js.js",
      "require": "./dist/cpt2js.cjs",
      "default": "./dist/cpt2js.js"
    }
  },
  "main": "./dist/cpt2js.cjs",
  "module": "./dist/cpt2js.js",
  "browser": "./dist/cpt2js.umd.min.js",
  "types": "./dist/cpt2js.d.ts",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "test": "tsx --test src/**/*.test.ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/chroma-js": "^2.4.3",
    "chroma-js": "^2.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
